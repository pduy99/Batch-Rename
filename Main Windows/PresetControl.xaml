<Window x:Class="Main_Windows.PresetControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Main_Windows"
        mc:Ignorable="d"
        Title="PresetControl" Height="120" Width="400">
    <Canvas>
        <TextBlock Text="Enter name to save: " FontWeight="SemiBold" Canvas.Left="19" Canvas.Top="18" />
        <TextBox Name="txtName" Width="200" Canvas.Left="138" Canvas.Top="18"/>
        <Button Click="btnOk" Width="80" Height="25" Content="Ok" Canvas.Left="155" Canvas.Top="55"/>
    </Canvas>
</Window>
